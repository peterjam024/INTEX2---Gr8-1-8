                        @Html.RadioButtonFor(model => model.PEDESTRIAN_INVOLVED, true) Yes
                    </div>
                    <div class="form-check">
                        @Html.RadioButtonFor(model => model.PEDESTRIAN_INVOLVED, false) No
                    </div>
                    <br />


                    <label asp-for="BICYCLIST_INVOLVED">Bicyclist Involved:</label>
