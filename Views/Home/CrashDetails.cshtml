                        @Html.RadioButtonFor(model => model.BICYCLIST_INVOLVED, true) Yes
                    </div>
                    <div class="form-check">
                        @Html.RadioButtonFor(model => model.BICYCLIST_INVOLVED, false) No
                    </div>
                    <br />

                    <label asp-for="MOTORCYCLE_INVOLVED">Motorcycle Involved:</label>
